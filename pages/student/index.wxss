.tabs {
    position:relative;
    padding: 180rpx 40rpx 40rpx 40rpx;
    height: 300rpx;
    background: url(http://img.uelink.com.cn/upload/xykj/student/navigation.png);
    background-size: 100% 300rpx;
}

.delImg{
    position:absolute;
    top:20rpx;
    right:40rpx;
    width:56rpx;
}

.tabs .bar {
    width: 100%;
    height: 80rpx;
    line-height: 80rpx;
}

.tabs .bar .hover {
    background: #0779E4;
    color: #fff;
    border-radius: 20rpx;
}

.panel {
    background: #F0F5F7;
    border-radius: 36rpx;
}


.drawer-box .text-custom{
    font-size:26rpx;
}

.drawer-box .input{
    height: 88rpx;
    line-height:88rpx;
    background: #F0F5F7;
    border-radius: 36rpx;
    text-align:center
}

.picker-shape {
    width:190rpx;
    font-size:26rpx;
}

.tabBgColor{
    background-color:#F0F5F7;
}

