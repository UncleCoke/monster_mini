<view class="bg flex flex-direction align-center">
    <view class="btn-sp">{{totalFee}}</view>
    <view class="text-white text-sm margin-top-xs">总收益（元）</view>
</view>
<view class="padding-lg solid-bottom text-sp text-bold">收益记录</view>
<empty wx:if="{{orderList.length == 0}}" title="暂无收益记录"></empty>
<view wx:for="{{orderList}}" wx:key="nickName" class="flex align-center">
    <view class="cu-avatar lg round margin-tb-lg margin-left-lg margin-right" style="background-image:url({{item.avatarUrl}})" wx:if="{{item.avatarUrl}}"></view>
    <view class="cu-avatar lg round margin-tb-lg margin-left-lg margin-right" wx:else>{{item.name[0]}}</view>
    <view class="flex align-center justify-between flex-sub padding-tb-lg padding-right-lg solid-bottom">
        <view class="flex-sub">
            <view class="text-sp">{{item.name}}</view>
            <view class="text-sm colorT-sp margin-top-sm">{{item.created_at}}</view>
        </view>
        <view class="text-sp text-bold margin-left-lg" style="color:#FFB341">{{item.totalFee}}元</view>
    </view>
</view>